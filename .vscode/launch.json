{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "start server",
    "protocol": "auto",
    "runtimeArgs": [
      "-r",
      "ts-node/register",
      "-r",
      "tsconfig-paths/register"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "cwd": "${workspaceFolder}/server",
    "args": [
      "${workspaceFolder}/server/src/main.ts"
    ]
  }]
}
