{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "start server",
    "runtimeArgs": [
      "-r",
      "ts-node/register",
      "-r",
      "tsconfig-paths/register"
    ],
    "env": {
      "MONGO_DB_HOSTNAME": "localhost",
      "MONGO_DB_PORT": "27017"
    },
    "cwd": "${workspaceFolder}/server",
    "args": [
      "${workspaceFolder}/server/src/main.ts"
    ]
  }]
}
